# Multimodal Descent 환경 설정
# .env.example 파일을 복사하여 .env로 만들고 실제 값으로 수정하세요

# Google Cloud Platform 설정
GCP_PROJECT_ID=your-project-id
GCP_PROJECT=your-project-id
BQ_DATASET=descent_demo
BQ_LOCATION=US

# Google Cloud Storage 설정 (선택사항)
GCS_BUCKET=gs://your-bucket-name

# 임베딩 모델 설정 (옵션 A: BigQuery AI 사용)
USE_REAL_EMBEDDINGS=true
EMBEDDING_MODEL=models.text_embedding
USE_MULTIMODAL=false

# ORI 알고리즘 파라미터
ORI_WEIGHT=0.7
ORI_THRESHOLD=0.3

# 성능 설정
TOP_K_RESULTS=10
BATCH_SIZE=1000
