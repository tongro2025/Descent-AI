# Multimodal Descent Environment Configuration
# Copy this file to .env and update with your actual values

# Google Cloud Platform Configuration
GCP_PROJECT=your-project-id
BQ_DATASET=descent_demo
BQ_LOCATION=US

# Google Cloud Storage Configuration (Optional)
GCS_BUCKET=gs://your-bucket-name

# Embedding Model Configuration (Option A: Using BigQuery AI)
USE_REAL_EMBEDDINGS=true
EMBEDDING_MODEL=models.text_embedding
USE_MULTIMODAL=false

# ORI Algorithm Parameters
ORI_WEIGHT=0.7
ORI_THRESHOLD=0.3

# Performance Settings
TOP_K_RESULTS=10
BATCH_SIZE=1000
